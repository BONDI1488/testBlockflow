{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"]
    },
    "strictNullChecks": false,
    "esModuleInterop": true
  },
  "include": [
    "src",
    ".eslintrc.js",
    "react-native.config.js",
    "metro.config.js",
    "index.js",
    "babel.config.js",
    "jest.config.js",
    "declarations.d.ts"
  ],
  "skipLibCheck": true
}
